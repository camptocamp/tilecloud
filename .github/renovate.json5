{
  $schema: 'https://docs.renovatebot.com/renovate-schema.json',
  extends: [
    'github>camptocamp/gs-renovate-config-preset:base.json5#1.8.1',
    'github>camptocamp/gs-renovate-config-preset:group.json5#1.8.1',
    'github>camptocamp/gs-renovate-config-preset:ci.json5#1.8.1',
    'github>camptocamp/gs-renovate-config-preset:preset.json5#1.8.1',
    'github>camptocamp/gs-renovate-config-preset:pre-commit.json5#1.8.1',
    'github>camptocamp/gs-renovate-config-preset:python.json5#1.8.1',
    'github>camptocamp/gs-renovate-config-preset:docker.json5#1.8.1',
    'github>camptocamp/gs-renovate-config-preset:json-schema.json5#1.8.1',
    'github>camptocamp/gs-renovate-config-preset:shellcheck.json5#1.8.1',
    'github>camptocamp/gs-renovate-config-preset:own.json5#1.8.1',
    'github>camptocamp/gs-renovate-config-preset:security.json5#1.8.1',
  ],
  packageRules: [
    {
      matchDatasources: ['docker'],
      versioning: 'regex:^(?<compatibility>.*)-(?<major>\\d+)\\.(?<minor>\\d+)\\.(?<patch>\\d+)?$',
      matchDepNames: ['ghcr.io/osgeo/gdal'],
      /** Ungroup Gdal */
      groupName: 'gdal',
    },
    /** Disable update of Python version in pyproject.toml */
    {
      matchFiles: ['pyproject.toml'],
      enabled: false,
      matchDepNames: ['python'],
    },
    /** Ungroup pycairo */
    {
      matchDepNames: ['pycairo'],
      groupName: 'pycairo',
    },
  ],
}
